
CREATE TABLE mts.records
(
    user_name character varying(50) COLLATE pg_catalog."default" NOT NULL,
    ticket_number character varying(15) COLLATE pg_catalog."default" NOT NULL,
    object_type character varying(50) COLLATE pg_catalog."default" NOT NULL,
    object_name character varying(150) COLLATE pg_catalog."default" NOT NULL,
    environments character varying(50) COLLATE pg_catalog."default" NOT NULL,
    record_id integer NOT NULL,
    action character varying(10) COLLATE pg_catalog."default" NOT NULL,
    CONSTRAINT records_pkey PRIMARY KEY (record_id)
)

